{
  "name": "awesome-github-app",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "prebuild": "bun run format && bun run lint:fix && bun run typecheck",
    "build": "expo export",
    "build:local": "expo run:android",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "format": "prettier . --write",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "expo start",
    "typecheck": "tsc --noEmit",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "55.0.0-preview.11",
    "expo-constants": "~55.0.6",
    "expo-linking": "~55.0.6",
    "expo-router": "~55.0.0-preview.9",
    "expo-status-bar": "~55.0.0",
    "expo-system-ui": "~55.0.0",
    "jiti": "^2.6.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/react": "~19.2.10",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^10.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.11",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.0"
  }
}
