{
  "name": "awesome-github-app",
  "version": "0.0.5",
  "private": true,
  "keywords": [
    "github",
    "app",
    "github-app",
    "github-client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/involvex/awesome-github-app.git"
  },
  "funding": {
    "url": "https://github.com/sponsors/involvex"
  },
  "author": {
    "name": "involvex"
  },
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo prebuild --platform android --no-install && expo run:android",
    "prebuild": "bun run format && bun run lint:fix && bun run typecheck",
    "build": "expo export",
    "build:local": "expo prebuild --platform android --no-install && expo run:android",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "check": "bun run prebuild && bun run expo:check && bun run doctor && bun run test && bun run test:ci",
    "clean": "del-cli dist build .expo .expo-shared node_modules workers/oauth-token-exchange/node_modules workers/oauth-token-exchange/.wrangler",
    "doctor": "bunx expo-doctor --verbose",
    "expo:check": "bunx expo install --check",
    "expo:prebuild": "bun run prebuild && expo prebuild",
    "format": "prettier . --write",
    "go": "bun run prebuild && bunx expo start --go -c",
    "ios": "expo run:ios",
    "lint": "eslint src --ignore-pattern .claude --ignore-pattern dist --ignore-pattern build --ignore-pattern workers --ignore-pattern __tests__",
    "lint:fix": "eslint --fix src --ignore-pattern .claude --ignore-pattern dist --ignore-pattern build --ignore-pattern workers --ignore-pattern __tests__",
    "native:sync": "expo prebuild --no-install",
    "start": "expo start",
    "test": "jest",
    "test:ci": "jest --runInBand --ci --coverage",
    "typecheck": "tsc --noEmit",
    "version:build": "expo-version-bump",
    "version:major": "expo-version-bump --major",
    "version:minor": "expo-version-bump --minor",
    "version:patch": "expo-version-bump --patch",
    "web": "expo start --web"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/build/",
      "__tests__/test-utils"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|expo(nent)?|expo-.*|expo-modules-core|expo-status-bar|expo-font|expo-asset|@expo|expo-router|@unimodules|@react-navigation|@tanstack|@expo/vector-icons|react-native-reanimated|react-native-gesture-handler|@octokit|universal-user-agent|before-after-hook|is-plain-object|react-native-markdown-display)/)"
    ]
  },
  "dependencies": {
    "@expo/config": "^55.0.1",
    "@octokit/graphql": "^9.0.3",
    "@octokit/rest": "^22.0.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@tanstack/react-query": "^5.90.21",
    "@testing-library/jest-native": "^5.4.3",
    "date-fns": "^4.1.0",
    "del-cli": "^7.0.0",
    "dotenv": "^17.3.1",
    "expo": "~55.0.4",
    "expo-auth-session": "~55.0.6",
    "expo-constants": "~55.0.7",
    "expo-crypto": "~55.0.8",
    "expo-dev-client": "~55.0.10",
    "expo-font": "~55.0.4",
    "expo-image": "~55.0.5",
    "expo-linking": "~55.0.7",
    "expo-notifications": "~55.0.10",
    "expo-router": "~55.0.3",
    "expo-secure-store": "~55.0.8",
    "expo-splash-screen": "~55.0.10",
    "expo-status-bar": "~55.0.4",
    "expo-system-ui": "~55.0.9",
    "expo-vector-icons": "^10.0.1",
    "expo-version-bump": "^1.0.2",
    "expo-web-browser": "~55.0.9",
    "jiti": "^2.6.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.7.2",
    "uniwind": "^1.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.2.10",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^10.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "jest": "~29.7.0",
    "jest-expo": "~55.0.9",
    "minimatch": "9.0.5",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.11",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "_tests_",
          "workers",
          "dist",
          "build",
          "node_modules",
          ".expo",
          ".expo-shared"
        ],
        "listUnknownPackages": true
      }
    }
  },
  "sponsor": {
    "url": "https://github.com/sponsors/involvex"
  }
}
