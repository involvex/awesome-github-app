{
  "files.exclude": {
    "out": false,
    "dist": false
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.dart_tool": true,
    "**/node_modules/*/**": true,
    "**/dist/*/**": true,
    "**/out/*/**": true
  },
  "search.exclude": {
    "out": true,
    "dist": true,
    "node_modules": true
  },
  "typescript.tsc.autoDetect": "off",
  "statusbarQuickActions.buttons": [
    {
      "id": "build",
      "text": "üî® build",
      "tooltip": "Run build command",
      "command": {
        "type": "bun",
        "script": "build"
      },
      "enabled": true,
      "alignment": "left",
      "priority": 100
    },
    {
      "id": "web",
      "text": "üåê web",
      "command": {
        "type": "bun",
        "script": "web"
      },
      "alignment": "left",
      "enabled": true,
      "priority": 100,
      "colors": {
        "background": "blue",
        "foreground": "black"
      },
      "tooltip": "run dev",
      "execution": {
        "showProgress": true,
        "notifications": {
          "showError": true
        }
      }
    },
    {
      "id": "changelog",
      "text": "üí¨ Changelog",
      "command": {
        "type": "bun",
        "script": "changelog"
      },
      "alignment": "left",
      "enabled": true,
      "priority": 100,
      "colors": {
        "background": "blue",
        "foreground": "black"
      },
      "tooltip": "run changelog",
      "execution": {
        "showProgress": true,
        "notifications": {
          "showError": true,
          "showOutput": true,
          "showSuccess": true
        }
      }
    }
  ],
  "statusbarQuickActions.settings.icons": {
    "library": "vscode"
  },
  "statusbarQuickActions.settings.theme": {
    "darkTheme": true
  },
  "npmPackageManager.defaultPackageManager": "bun",
  "npmPackageManager.debug": true,
  "npmPackageManager.showStatusBarItem": true,
  "DockerRun.DisableAutoGenerateConfig": true,
  "cSpell.words": [
    "awesomegithubapp",
    "bunx",
    "KNCY",
    "magick",
    "mmkv",
    "prebuild",
    "Uniwind",
    "Unstar",
    "Unwatch",
    "worklets"
  ],
  "snyk.advanced.organization": "d20b9212-7c2e-4cf5-9fbd-bd394b0b616f",
  "snyk.advanced.autoSelectOrganization": true
}
